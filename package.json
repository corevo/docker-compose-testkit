  "dependencies": {
    "@types/js-yaml": "^4.0.5",
    "execa": "^6.0.0",
    "faker": "^5.5.3",
    "got": "^12.0.0",
    "js-yaml": "^4.1.0",
    "p-retry": "^5.0.0"
  },
  "devDependencies": {
    "@corevo/docker-compose-tool": "^1.4.3",
    "@jest/globals": "^27.2.5",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  }
