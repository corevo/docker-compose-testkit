    "p-retry": "^6.0.0",
    "supports-color": "^9.2.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.0.0",
    "@types/debug": "^4.1.7",
    "@types/js-yaml": "^4.0.5",
